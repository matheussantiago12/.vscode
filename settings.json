{
  "editor.fontSize": 17,
  "editor.lineHeight": 1.5,
  "editor.parameterHints.enabled": false,
  "editor.renderLineHighlight": "none",
  "editor.suggestSelection": "first",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.semanticHighlighting.enabled": false,
  "editor.fontFamily": "JetBrains Mono, Ubuntu Mono",
  // "editor.fontLigatures": "'calt', 'liga', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08', 'ss09'",
  "editor.fontLigatures": true,
  "editor.fontWeight": "500",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.minimap.autohide": true,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.guides.indentation": true,
  "editor.glyphMargin": true,
  "editor.showFoldingControls": "never",
  "editor.lightbulb.enabled": "off",
  "editor.cursorStyle": "block",
  "editor.cursorBlinking": "solid",
  "editor.mouseWheelScrollSensitivity": 1.6,
  "editor.stickyScroll.enabled": false,
  "editor.lineDecorationsWidth": 22,
  "editor.matchBrackets": "never",
  "editor.occurrencesHighlight": "off",
  "workbench.tree.renderIndentGuides": "none",
  "workbench.editor.labelFormat": "short",
  "workbench.productIconTheme": "fluent-icons",
  "workbench.editor.empty.hint": "hidden",
  "workbench.activityBar.location": "top",
  "workbench.editor.editorActionsLocation": "hidden",
  "workbench.colorTheme": "Vim Dark Hard",
  "workbench.startupEditor": "none",
  "workbench.colorCustomizations": {
    "editorRuler.foreground": "#ffffff0a",
    "editorIndentGuide.activeBackground1": "#ffffff07",
    "editorIndentGuide.background1": "#ffffff01",
    "[Dainty â€“ Panda Theme (chroma 1, lightness 2)]": {
      "editorGutter.modifiedBackground": "#00ddff9a",
      "editorGutter.addedBackground": "#00ff6a9d",
      "editorGutter.deletedBackground": "#ff00006b"
    },
    "tab.activeBorder": "#00000000",
    "tab.activeBackground": "#ffffff0c",
  },
  "workbench.iconTheme": "quill-icons",
  "workbench.editor.enablePreview": false,
  "window.titleBarStyle": "custom",
  "javascript.suggest.autoImports": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.tsserver.log": "off",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDelete": false,
  "terminal.integrated.showExitAlert": false,
  "typescript.suggest.autoImports": true,
  "explorer.compactFolders": false,
  "explorer.confirmDragAndDrop": false,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontFamily": "JetBrains Mono",
  "[java]": {
    "editor.tabSize": 4
  },
  "security.workspace.trust.untrustedFiles": "newWindow",
  "update.mode": "start",
  "terminal.integrated.gpuAcceleration": "off",
  "terminal.integrated.defaultProfile.osx": "fish",
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "git.openRepositoryInParentFolders": "always",
  "symbols.hidesExplorerArrows": false,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "update.showReleaseNotes": false,
  "security.promptForLocalFileProtocolHandling": false,
  "zenMode.silentNotifications": true,
  "zenMode.centerLayout": false,
  "zenMode.fullScreen": false,
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false,
  "zenMode.showTabs": "none",
  "window.commandCenter": false,
  "window.customTitleBarVisibility": "auto",
  "turboConsoleLog.addSemicolonInTheEnd": true,
  "turboConsoleLog.includeFileNameAndLineNum": true,
  "turboConsoleLog.insertEnclosingFunction": false,
  "turboConsoleLog.insertEnclosingClass": false,
  "turboConsoleLog.quote": "'",
  "turboConsoleLog.delimiterInsideMessage": "|",
  "turboConsoleLog.logMessagePrefix": "ðŸ”´",
  "vsnotes.defaultNotePath": "/home/desenvolvimento/Documentos/notes",
  "vsnotes.defaultNoteTitle": "{title}.{ext}",
  "vsnotes.listRecentLimit": 30,
  "vsnotes.noteTitleConvertSpaces": " ",
  "window.menuBarVisibility": "toggle",
  "window.title": "${rootName}",
  "scss.validate": false,
  "window.zoomLevel": -0.1,
  "files.exclude": {
    ".circleci": true,
    ".github": true,
    ".vscode": false,
    "dist": true,
    // "node_modules": true
  },
  "extensions.ignoreRecommendations": false,
  "explorer.confirmPasteNative": false,
  "git.confirmSync": false,
  "redhat.telemetry.enabled": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "files.associations": {
    "*.md": "note"
  },
  "github.copilot.enable": {
    "*": true,
    "markdown": false,
    "mdx": false,
    "md": false
  },
  // APC
  "apc.font.family": "Inter",
  "apc.electron": {
    "frame": false,
    "titleBarStyle": "hiddenInset",
  },
  "apc.listRow": {
    "height": 21.5,
  },
  "apc.stylesheet": {
    ".quick-input-widget": "top: 25% !important; padding: 6px 6px 0px 6px !important;",
    ".quick-input-widget .monaco-inputbox": "padding:3px !important; margin-bottom: 5px;",
    ".explorer-viewlet": "font-family: 'JetBrains Mono' !important; font-weight: 400 !important; scrollbar-width: 0px !important;",
    ".title-label > h2": "opacity: 0.7 !important;",
    ".codicon-toolbar-more": "opacity: 0.6 !important;",
    ".action-toolbar-container": "opacity: 0.5 !important;",
    ".action-toolbar-container:hover": "opacity: 0.8 !important;",
    ".window-controls-container": "opacity: 0.5 !important;",
    ".window-controls-container:hover": "opacity: 0.8 !important;",
    ".window-title": "opacity: 0.75 !important;",
    ".action-item.icon.checked a": "color: rgb(170, 170, 170) !important;",
    ".action-item.icon a": "color: rgb(100, 100, 100) !important;",
    ".explorer-folders-view .slider": "width: 0px !important;"
  },
  // GitLens
  "gitlens.hovers.currentLine.over": "annotation",
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.changes": false,
  "gitlens.hovers.currentLine.details": true,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.advanced.messages": {
    "suppressCreatePullRequestPrompt": true
  },
  // "vetur.format.defaultFormatter.ts": "none",
  "editor.renderWhitespace": "none",
  "terminal.integrated.env.linux": {
    "JAVA_HOME": "/usr/lib/jvm/java-1.17.0-openjdk-amd64",
    "PATH": "/usr/lib/jvm/java-1.17.0-openjdk-amd64/bin:${env:PATH}"
  },
  "breadcrumbs.enabled": false,
  "explorer.decorations.colors": false,
  "workbench.tree.enableStickyScroll": false,
  "ActiveFileInStatusBar.enable": true,
  "ActiveFileInStatusBar.fullpath": false,
  "ActiveFileInStatusBar.revealFile": true,
  "workbench.editor.showTabs": "none",
  "window.newWindowDimensions": "inherit",
  "workbench.layoutControl.enabled": false,
  // "ActiveFileInStatusBar.color": "rgba(255,255,255,0.45)",
  // "vim.handleKeys": {
  //   "<C-p>": false
  // },
	"catppuccin-icons.hidesExplorerArrows": true,
}
